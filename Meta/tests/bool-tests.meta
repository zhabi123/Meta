(tests
 [(and (eq? 0 0) (eq? 1 1) (eq? 2 2)) #t]
 [(and (eq? 0 0) (eq? 0 1) (eq? 2 2)) #f]

 [(or (eq? 1 0) (eq? 0 1) (eq? 1 1)) #t]
